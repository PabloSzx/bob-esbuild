{
  "name": "basic",
  "version": "1.0.0",
  "private": true,
  "license": "MIT",
  "author": "PabloSzx <pablosaez1995@gmail.com>",
  "exports": {
    ".": {
      "require": "./lib/index.js",
      "import": "./lib/index.mjs"
    },
    "./*": {
      "require": "./lib/*.js",
      "import": "./lib/*.mjs"
    },
    "./other": "./lib/other.js",
    "./other_other": {
      "require": "./lib/other.js",
      "import": "./lib/other.mjs"
    },
    "./package.json": "./package.json"
  },
  "main": "lib/index.js",
  "module": "lib/index.mjs",
  "types": "lib/index.d.ts",
  "bin": {
    "test_cli": "bin/index.js"
  },
  "files": [
    "lib"
  ],
  "scripts": {
    "dev": "bob-esbuild watch --onlyCJS",
    "prepare": "bob-esbuild build && bob-tsm lib/index.js"
  },
  "dependencies": {
    "bob-esbuild-cli": "workspace:^3.0.0",
    "bob-tsm": "workspace:^0.3.5",
    "shared": "workspace:^1.0.0"
  },
  "publishConfig": {
    "directory": "lib"
  },
  "buildConfig": {
    "bin": {
      "test_cli": {
        "input": "src/other.ts"
      }
    }
  }
}
