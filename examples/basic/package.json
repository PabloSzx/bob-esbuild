{
  "name": "basic",
  "version": "1.0.0",
  "private": true,
  "license": "MIT",
  "author": "PabloSzx <pablosaez1995@gmail.com>",
  "exports": {
    ".": {
      "require": "./lib/index.js",
      "import": "./lib/index.mjs"
    },
    "./*": {
      "require": "./lib/*.js",
      "import": "./lib/*.mjs"
    }
  },
  "main": "lib/index.js",
  "module": "lib/index.mjs",
  "types": "lib/index.d.ts",
  "bin": {
    "test_cli": "bin/index.js"
  },
  "files": [
    "lib"
  ],
  "scripts": {
    "dev": "bob-esbuild watch",
    "prepare": "bob-esbuild build && node lib/index.js"
  },
  "dependencies": {
    "bob-esbuild-cli": "workspace:^0.2.2",
    "shared": "workspace:^1.0.0"
  },
  "buildConfig": {
    "bin": {
      "test_cli": {
        "input": "src/other.ts"
      }
    }
  },
  "publishConfig": {
    "directory": "lib"
  }
}
