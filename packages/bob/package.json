{
  "name": "bob-esbuild",
  "version": "1.0.0",
  "license": "MIT",
  "author": "PabloSzx <pablosaez1995@gmail.com>",
  "exports": {
    ".": {
      "require": "./lib/index.js",
      "import": "./lib/index.mjs"
    },
    "./*": {
      "require": "./lib/*.js",
      "import": "./lib/*.mjs"
    }
  },
  "main": "lib/index.js",
  "module": "lib/index.mjs",
  "types": "lib/index.d.ts",
  "scripts": {
    "postpack": "rimraf oclif.manifest.json",
    "prepare": "node -r sucrase/register self-build.ts"
  },
  "dependencies": {
    "builtin-modules": "^3.2.0",
    "esbuild": "^0.12.4",
    "fs-extra": "^10.0.0",
    "globby": "^11.0.3",
    "oclif": "^1.18.0",
    "rimraf": "^3.0.2",
    "rollup": "^2.50.1",
    "rollup-plugin-delete": "^2.0.0",
    "rollup-plugin-esbuild": "^4.2.3",
    "rollup-plugin-node-externals": "^2.2.0"
  },
  "devDependencies": {
    "@types/fs-extra": "^9.0.11",
    "@types/rimraf": "^3.0.0",
    "sucrase": "^3.18.1",
    "typescript": "^4.3.2"
  },
  "peerDependencies": {
    "typescript": "^4.3.2"
  },
  "typesVersions": {
    "*": {
      "*": [
        "lib/*"
      ]
    }
  }
}
