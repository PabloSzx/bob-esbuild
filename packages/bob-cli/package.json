{
  "name": "bob-esbuild-cli",
  "version": "0.1.14",
  "homepage": "https://github.com/PabloSzx/bob-esbuild",
  "bugs": "https://github.com/PabloSzx/bob-esbuild/issues",
  "repository": "https://github.com/PabloSzx/bob-esbuild",
  "license": "MIT",
  "author": "PabloSzx <pablosaez1995@gmail.com>",
  "exports": {
    ".": {
      "require": "./lib/index.js",
      "import": "./lib/index.mjs"
    },
    "./*": {
      "require": "./*.js",
      "import": "./*.mjs"
    }
  },
  "main": "lib/index.js",
  "module": "lib/index.mjs",
  "types": "lib/index.d.ts",
  "bin": {
    "bob-esbuild": "./bin/run.js"
  },
  "files": [
    "/bin",
    "/lib"
  ],
  "scripts": {
    "dev": "bob-esbuild watch",
    "prepare": "node -r esbuild-register self-build.ts",
    "postpublish": "gh-release --repo PabloSzx/bob-esbuild"
  },
  "dependencies": {
    "@oclif/command": "^1.8.0",
    "@oclif/config": "^1.17.0",
    "@oclif/errors": "^1.3.4",
    "@oclif/parser": "^3.8.5",
    "@oclif/plugin-help": "^3.2.2",
    "bob-esbuild": "workspace:^0.1.11"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.26.0",
    "@types/node": "^15.6.1",
    "changesets-github-release": "^0.0.1",
    "esbuild-register": "^2.5.0",
    "globby": "^11.0.3",
    "rimraf": "^3.0.2",
    "ts-node": "^10.0.0",
    "typescript": "^4.3.2"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "oclif": {
    "commands": "./lib/commands",
    "bin": "bob",
    "plugins": [
      "@oclif/plugin-help"
    ]
  }
}
